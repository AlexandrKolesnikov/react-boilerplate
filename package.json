{
  "name": "react-pwa-boilerplate",
  "version": "1.0.0",
  "description": "React PWA Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env.ENVIRONMENT_FILE_NAME=.env.development --config webpack/webpack.config.js",
    "build": "cross-env npm run clean && webpack --env.ENVIRONMENT_FILE_NAME=.env --config webpack/webpack.production.config.js --progress --profile --colors --debug",
    "serve": "cross-env babel-node server/index.js serve",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./stories",
    "lint-fix": "npm run lint -- --fix",
    "stylelint": "stylelint src/**/*.{scss,css}",
    "stylelint-fix": "npm run stylelint -- --fix",
    "check-types": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types --silent && lint-staged"
    }
  },
  "author": "Alexandr Kolesnikov",
  "license": "MIT",
  "browserslist": [
    "last 20 version"
  ],
  "dependencies": {
    "@storybook/addon-info": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "classnames": "^2.2.6",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "npm-check-updates": "^3.1.24",
    "object-diff": "0.0.4",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.6",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.1"
  },
